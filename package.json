{
  "name": "minimarkets",
  "version": "0.1.0",
  "description": "Api minimarkert",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon  --ignore thunder/ --watch src/ --ignore src/db/migrations src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrations:generate": "sequelize migration:generate --name",
    "migrations:run": "sequelize db:migrate --debug",
    "migrations:revert": "sequelize db:migrate:undo",
    "migrations:delete": "sequelize db:migrate:undo:all"
  },
  "keywords": [
    "market",
    "mini",
    "api"
  ],
  "author": "ricardo.pereze.98@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.1",
    "nodemailer": "^6.9.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.33.0",
    "uuid": "^9.0.1"
  },
